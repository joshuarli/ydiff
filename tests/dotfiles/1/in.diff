-	result  = ''
-	escaped = False
-	for i, c in enumerate(args):
-		if not escaped:
-			if c == '\\':
-				escaped = True
-				continue
-			elif c == ',':
-				return result, args[i+1:]
-		result  += c
-		escaped  = False
-	return result, None
