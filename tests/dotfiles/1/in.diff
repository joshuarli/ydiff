+def on_signal(*args, **kwargs):
+	global timer
+	''' Called whenever the buffer list changes. '''
+	if timer is not None:
+		weechat.unhook(timer)
+		timer = None
+	weechat.hook_timer(config.signal_delay, 0, 1, "on_timeout", "")
+	return weechat.WEECHAT_RC_OK
