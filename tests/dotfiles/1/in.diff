-    for buffer_name in buffer_names:
-        if buffer_name and buffer_name in ignored_buffers():
-            return True
-
-    for buffer_name in buffer_names:
-        for prefix in ignored_buffer_prefixes():
-            if prefix and buffer_name and buffer_name.startswith(prefix):
-                return True
